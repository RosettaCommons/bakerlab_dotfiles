[credential]
	helper = cache --timeout=3600
[core]
	autocrlf = input
	excludesfile = /Users/weitzner/.gitignore_global
[push]
	default = simple
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[difftool]
	        prompt = false
[alias]
	        ldiff = difftool -t latex
